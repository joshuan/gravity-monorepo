{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react",

    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",

    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "noUncheckedIndexedAccess": true
  }
}
